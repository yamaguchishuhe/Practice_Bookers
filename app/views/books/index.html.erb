<body>
  <p id="notice"></p>
  <h1>Books</h1>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>body</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  <%= @books. each do |book| %>
  <tbody>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td><%= link_to "show", books_path(@book.id)</td>
      <td><%= link_to "edit", books_edit_path(@book.id)</td>
      <td><%= link_to "destroy", destroy_books_path(@book.id)</td>
    </tr>
  </tbody>
  <% end %>
  </table>
  <h2>New book</h2>
  <%= form_with model:@bolg, local:true do  |f| %>
  <h4>title</h4>
  <%= f.text_field :title %>
  <h4>body</h4>
  <%= f.text_field :body %>

  <%= f.submit 'Create Book' %>
</body>